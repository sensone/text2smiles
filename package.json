{
  "name": "text2smiles",
  "version": "1.1.1",
  "description": "Converting text to smiles",
  "keywords": [
    "emoji",
    "smiles",
    "text to emoji",
    "text to symbols",
    "converting text"
  ],
  "main": "./lib/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./src",
    "transpile-es2015": "babel src -d lib",
